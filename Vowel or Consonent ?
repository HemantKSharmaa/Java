import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
      char characters;
      Scanner hks = new Scanner(System.in);
      characters = hks.next().charAt(0);
      
      switch(characters){
        
        case 'a':
          System.out.println("vowel");
          break;
          
        case 'e':
          System.out.println("vowel");
          break;
          
        case 'i':
          System.out.println("vowel");
          break;
          
        case 'o':
          System.out.println("vowel");
          break;
          
        case 'u':
          System.out.println("vowel");
          break;
          
        default:
          System.out.println("consonant");
          break;
      }
      
      
  }
}
