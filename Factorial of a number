import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
      int n,fac=1;
      
      Scanner input = new Scanner(System.in);
      n = input.nextInt();
      
      for(int i=1; i<=n; i++){
        fac=fac*i;
        
      }
      System.out.println(fac);
  }
}
