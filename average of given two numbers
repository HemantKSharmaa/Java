import java.util.Scanner;
public class solution{
  public static void main(String args[]){
    int a,b,c;
    
    Scanner input = new Scanner(System.in);
    a = input.nextInt();
    b = input.nextInt();
    c = input.nextInt();
    
    
    int average = (a+b+c)/3 ;
    
    System.out.println(average);
    
    }
}
